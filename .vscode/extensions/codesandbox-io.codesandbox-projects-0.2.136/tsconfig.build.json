{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "rootDir": ".",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["typings", "src"],
  "exclude": ["node_modules", ".vscode-test", "out"]
}
